.StarInfo {
    id 0: integer
    finish 1: integer   #0 no 1 yes
    star 2: integer     #star num
}

.BattleDropItem {
    id 0 : integer
    amount 1 : integer
}

.BattleDrop {
    normal 0: *BattleDropItem
    boss 1: *BattleDropItem
}

.BattleSettle {
    exp 0 : integer
    gold 1 : integer
    star_map 2 : *StarInfo
    items 3 : *BattleDropItem
}

.BattleEvent {
    id 0: integer
    data 1: string
}

