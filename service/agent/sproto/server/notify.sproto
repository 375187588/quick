# [notify] ------ [1, 100) ------
daily_update 1 {}

chat 2 { 
    request {
        chats 0 : *Chat
    }   
}

mail 3 {
    request {
        private_mails 0: *Mail   #私人邮件
        system_mails 1: *Mail   #系统邮件
    }
}

notify_udp_addr 4 {
    request {
        udp_ip 0: string
        udp_port 1: integer
    }
}

room_action 5 { 
    request {
        action 0 : integer # 1 enter 2 exit
        uuid 1 : string
    }   
}

battle_team_apply_result 6 {
    request {
        flag 0: boolean
    }
}

battle_server_event 7 {
    request {
        event 0: BattleEvent
    }
}

battle_control_action 8 {
    request {
        uuid 0: string        #nil代表没人控制
        control_list 1: *integer
    }
}

